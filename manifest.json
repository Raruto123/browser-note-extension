{
    "manifest_version": 3,
    "name": "Browser Notes",
    "version": "1.0.0",
    "description": "Remind you things you noted on websites",
    "permissions" : ["activeTab"],
    "action": {
        "default_popup" : "base-popup.html",
        "default_icon" : {
            "16": "browser-notes 16.png",
            "48": "browser-notes.png",
            "128": "browser-notes 128.png"
        }
    }
  }